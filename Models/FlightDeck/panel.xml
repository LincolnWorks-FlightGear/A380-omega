<PropertyList>
	<path>fd_panel.ac</path>
	
	<!-- Animations -->
	
	<!-- Gear Level -->
	<animation>
		<type>rotate</type>
		<object-name>lever.gear</object-name>
		<property>/controls/gear/gear-down</property>
		<interpolation>
			<entry><ind>0</ind><dep>-30</dep></entry>
			<entry><ind>1</ind><dep>30</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.8532</x-m>
			<y-m>0</y-m>
			<z-m>-0.3624</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>lever.gear</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>/controls/gear/gear-down</property>
			</binding>
			<binding>
                  <command>property-toggle</command>
                  <property>/sim/sound/switch</property>
            </binding>
		</action>
	</animation>

	<!-- GEAR Situation Indicators -->
	
	<animation>
		<type>textranslate</type>
		<object-name>ind.gear.0</object-name>
		<property>/fdm/jsbsim/gear/unit/pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.01</ind><dep>0</dep></entry>
			<entry><ind>0.011</ind><dep>0.25</dep></entry>
			<entry><ind>0.95</ind><dep>0.25</dep></entry>
			<entry><ind>0.951</ind><dep>0.50</dep></entry>
			<entry><ind>0.999</ind><dep>0.50</dep></entry>
			<entry><ind>1</ind><dep>0.75</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
		</axis>
		<condition>
			<property>/systems/electric/outputs/avionics</property>
		</condition>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>ind.gear.1</object-name>
		<property>/fdm/jsbsim/gear/unit[1]/pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.01</ind><dep>0</dep></entry>
			<entry><ind>0.011</ind><dep>0.25</dep></entry>
			<entry><ind>0.95</ind><dep>0.25</dep></entry>
			<entry><ind>0.951</ind><dep>0.50</dep></entry>
			<entry><ind>0.999</ind><dep>0.50</dep></entry>
			<entry><ind>1</ind><dep>0.75</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
		</axis>
		<condition>
			<property>/systems/electric/outputs/avionics</property>
		</condition>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>ind.gear.4</object-name>
		<property>/fdm/jsbsim/gear/unit[2]/pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.01</ind><dep>0</dep></entry>
			<entry><ind>0.011</ind><dep>0.25</dep></entry>
			<entry><ind>0.95</ind><dep>0.25</dep></entry>
			<entry><ind>0.951</ind><dep>0.50</dep></entry>
			<entry><ind>0.999</ind><dep>0.50</dep></entry>
			<entry><ind>1</ind><dep>0.75</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
		</axis>
		<condition>
			<property>/systems/electric/outputs/avionics</property>
		</condition>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>ind.gear.2</object-name>
		<property>/fdm/jsbsim/gear/unit[3]/pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>0.01</ind><dep>0</dep></entry>
			<entry><ind>0.011</ind><dep>0.25</dep></entry>
			<entry><ind>0.95</ind><dep>0.25</dep></entry>
			<entry><ind>0.951</ind><dep>0.50</dep></entry>
			<entry><ind>0.999</ind><dep>0.50</dep></entry>
			<entry><ind>1</ind><dep>0.75</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
		</axis>
