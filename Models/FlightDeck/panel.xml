	<!-- Autobrake system -->
	<animation>
		<type>rotate</type>
		<object-name>knob.autobrk</object-name>
		<property>/hydraulics/brakes/autobrake-setting</property>
		<interpolation>
			<entry><ind>0</ind><dep>-135</dep></entry>
			<entry><ind>1</ind><dep>-90</dep></entry>
			<entry><ind>2</ind><dep>-45</dep></entry>
			<entry><ind>3</ind><dep>0</dep></entry>
			<entry><ind>4</ind><dep>45</dep></entry>
			<entry><ind>5</ind><dep>90</dep></entry>
		</interpolation>
		<axis>
			<x1-m>-0.8588</x1-m>
			<y1-m>0.2234</y1-m>
			<z1-m>-0.2858</z1-m>
			<x2-m>-0.8614</x2-m>
			<y2-m>0.2234</y2-m>
			<z2-m>-0.2865</z2-m>
		</axis>
	</animation>
	<animation>
	   <type>pick</type>
	  <object-name>knob.autobrk</object-name>
	  <action>
	    <button>0</button>
	    <button>3</button>
	    <repeatable>false</repeatable>
	    <binding>
		<command>property-adjust</command>
		<property>/hydraulics/brakes/autobrake-setting</property>
		<step>1</step>
		<min>0</min>
		<max>5</max>
		<wrap>false</wrap>
	    </binding>
	    <binding>
		  <command>property-toggle</command>
		  <property>/sim/sound/switch</property>
	      </binding>
	  </action>
	  <action>
	      <button>1</button>
	      <button>4</button>
	      <repeatable>false</repeatable>
	      <binding>
		  <command>property-adjust</command>
		  <property>/hydraulics/brakes/autobrake-setting</property>
		  <step>-1</step>
		  <min>0</min>
		  <max>5</max>
		  <wrap>false</wrap>
	    </binding>
	    <binding>
		<command>property-toggle</command>
		<property>/sim/sound/switch</property>
	    </binding>
	  </action>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>btn.autobrk</object-name>
		<property>/hydraulics/brakes/autobrake-rto</property>
		<factor>0.25</factor>
		<axis>
			<x>1</x>
		</axis>
		<condition>
			<property>/systems/electric/outputs/avionics</property>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>btn.autobrk</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>/hydraulics/brakes/autobrake-rto</property>
			</binding>
			<binding>
                  <command>property-toggle</command>
                  <property>/sim/sound/switch</property>
            </binding>
		</action>
	</animation>
	
	<!-- Navigation Display and Vertical Situation Display -->
	<animation>
        <type>select</type>
        <object-name>nd.l</object-name>
        <object-name>vsd.l</object-name>
        <object-name>nd.r</object-name>
        <object-name>vsd.r</object-name>
        <object-name>ewd</object-name>
        <condition>
            <equals>
                <property>systems/electric/outputs/avionics</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    
    <animation>
		<type>pick</type>
		<object-name>nd.l</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>nd.showNd();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>nd.r</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>nd.showNd("fo");</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>vsd.l</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>nd.capt_vsd.showDlg();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>vsd.r</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>nd.fo_vsd.showDlg();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>ewd</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>ecam.ewd.showDlg();</script>
			</binding>
		</action>
	</animation>
	
</PropertyList>
